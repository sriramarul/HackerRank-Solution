#include<bits/stdc++.h>
using namespace std;

int main() {
    int T;
    cin>>T;
    for(int i=0;i<T;i++){
        int N;
        cin>>N;
        vector<int> a;
        for(int o=0;o<N-1;o++){
            int b;
            cin>>b;
            a.push_back(b);
          }
        sort(a.begin(),a.end());
        int u=0;
        int size=a.size();
        if(a[size-1]==N-1){
            cout<<N<<endl;
        }else{
        for(int k=1;k<=N-1;k++){
           
           if(k==a[u]){
        
                u++;
            }else{
                cout<<k<<endl;
                break;
            }
        }}
    }

	return 0;
}
