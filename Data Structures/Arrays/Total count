#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
	   int n,mcount=0,th,thres;
	   cin>>n;
	   cin>>thres;
	   for(int i=0;i<n;i++){
	       int b;
	       cin>>b;
	       int count=0;
	       if(b%thres==0){
	           mcount=mcount+(b/thres);
	       }else{
	           mcount=mcount+(b/thres)+1;
	       }
	   }
	   cout<<mcount<<endl;
	    }
	return 0;
}
	
