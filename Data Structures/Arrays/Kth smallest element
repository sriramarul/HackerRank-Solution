#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    cout.tie(nullptr);
	int T;
	cin>>T;
	for(int i=0;i<T;i++){
	    int N;
	    cin>>N;
	    vector<int> a;
	    for(int j=0;j<N;j++){
	        int b;
	        cin>>b;
	        a.push_back(b);
	        
	    }
	    int k;
	    cin>>k;
	    sort(a.begin(),a.end());
	    cout<<a[k-1]<<endl;
	}
	return 0;
}
