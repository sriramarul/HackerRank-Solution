/*
struct Node {
    int data;
    struct Node *next;
    Node(int x) {
        data = x;
        next = NULL;
    }
*/
int detectloop(Node *head) {
    
    Node* fast=head;
    Node* slow=head;
    while(fast->next!=NULL && fast->next->next!=NULL){
        fast=fast->next;
        fast=fast->next;
        slow=slow->next;
        if(slow==fast){
            return 1;
        }

    }
    return 0;
    
}
