#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    int t;
    cin>>t;
    for(int j=0;j<t;j++){
    int flag=0;
    string s;
    cin>>s;
    for(int i=s.length()-1;i>=0;i--){
        if(s[i]=='1'){
            cout<<i<<"\n";
            flag=1;
            break;
        }
    }
    if(flag==0){
        cout<<-1<<"\n";
    }
    }
	return 0;
}
