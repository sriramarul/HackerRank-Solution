#include <iostream>
#include <string.h>
using namespace std;

int main() {
    int T,size=0;
    cin>>T;
    for(int i=0;i<T;i++){
       string s1,s2;
       cin.ignore();
       getline(cin,s1);
       cin>>size;
       
       for(int j=0;j<s1.length();j++)
       {if(s1[j]==' '){ s2.push_back('%');
           s2.push_back('2');
           s2.push_back('0');
       }else{s2.push_back(s1[j]);}}
       cout<<s2<<endl;
       
       
   }
    
	return 0;
}
